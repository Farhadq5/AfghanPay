    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>7c08fff3-ed6c-48e2-974c-ed0ba46db436</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="WebUI\admin_app.html" />
    <Content Include="WebUI\gent_app.html" />
